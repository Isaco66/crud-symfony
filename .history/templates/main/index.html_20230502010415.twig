{% extends 'base.html.twig' %}

{% block title %}Hello MainController!
{% endblock %}

{% block body %}

	<div class="container">
		<table class="table table-striped table-inverse table-responsive">
			<thead class="thead-inverse">
				<tr>
					<th>ID</th>
					<th>Title</th>
					<th>Content</th>
				</tr>
			</thead>
			<tbody>
				{% for dataIndividual in data %}
					<tr>
						<td scope="row">{dataIndividual.id}</td>
						<td>{dataIndividual.title}</td>
						<td>{dataIndividual.content}</td>
					</tr>
				{% endfor %}
			</tbody>
		</table>
	</div>


{% endblock %}

