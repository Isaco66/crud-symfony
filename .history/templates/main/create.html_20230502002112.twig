{% extends "base.html.twig" %}

{% block body %}

	<div class="container">
		<div class="row">
			<h3 class="mt-5 mb-5">Create Form</h3>
			<div class="col-md-6 offset-md-3">
                {{form_start(form)}}
                    <div class="form-group">
                      {{form_label(form.title)}}
                      {{form_widget(form.title,{'attr':{'class':'form-control'}})}}
                      
                    </div>
                    <div class="form-group">
                      <label for="">Content</label>
                      <input type="text" name="content" id="id_content" class="form-control" placeholder="" aria-describedby="helpId">
                    </div>
                    <div class="form-group">
                        <button class="btn btn-info text-white mt-4">submit </button>
                    </div>
                {{form_end(form)}}
            </div>
		</div>
	</div>


{% endblock %}
